System.out.print("nhập a: ");
float a=sc.nextFloat();
System.out.print("nhập b: ");
float b=sc.nextFloat();
float x;
if(a==0) {
if(b==0) {
System.out.print("phương trình vô số nghiệm ");
}else {
System.out.print("phương trình vô nghiệm ");
}
}else {
System.out.print("x= "+(-b/a));
}
}
}




3.//xác định số N có phải là số nguyên tố hay không
package inforstudent;
import java.util.Scanner;
public class nguyento {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
System.out.print("nhập N= ");
int n=sc.nextInt();
if (n <= 1) {
System.out.println("N không là số nguyên tố");
return;
}

boolean laSoNguyenTo = true;
for (int i = 2; i <= Math.sqrt(n); i++) {
if (n % i == 0) {
laSoNguyenTo = false;
break;
}
}

if (laSoNguyenTo) {
System.out.println("N là số nguyên tố");
} else {
System.out.println("N không là số nguyên tố");
}
}
}



4.//tính giai thừa
package inforstudent;
import java.util.Scanner;
public class giaithua {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
System.out.print("nhập N= ");
int n=sc.nextInt();
int s=n;
if(n==0) {
System.out.print("không thể tính được giai thừa");
}
for(int i=1;i<=n-2;i++) {
s=s*(n-i);
}
System.out.print("N!= "+s);
}

}


5.//phương trình bậc 2 ax^2+bx+c=0
package inforstudent;
import java.util.Scanner;
public class ptbac2 {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
System.out.print("nhập a: ");
float a=sc.nextFloat();
System.out.print("nhập b: ");
float b=sc.nextFloat();
System.out.print("nhập c: ");
float c=sc.nextFloat();
float delta=(float) (b*b-4*a*c);
if(delta==0) {
System.out.print("x= "+(-b/(2*a)));
}else if(delta <0) {
System.out.print("phương trình vô nghiệm");
}else if(delta >0) {
float x1=(float)((-b + Math.sqrt(delta))/(2*a));
float x2=(float)((-b - Math.sqrt(delta))/(2*a));
System.out.print("x1= "+x1);
System.out.print("\nx2= "+x2);
}
}

}


6.//tìm số lớn nhất trong mảng số nguyên
package inforstudent;
import java.util.Scanner;
public class maxinmang {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
System.out.print("nhập N= ");
int n=sc.nextInt();
int []a=new int[n];
for(int i=0;i<n;i++) {
System.out.print("nhập phần tử thứ "+i+":");
a[i]=sc.nextInt();
}
int max=a[0];
for(int i=1;i<n;i++) {
if(a[i]>max) {
max=a[i];
}
}
System.out.print("phần tử lớn nhất trong mảng là: "+max);
}
}



7.//in số chẵn trong mảng
package inforstudent;
import java.util.Scanner;
public class insochantrongmang {

public static void main(String[] args) {
Scanner sc = new Scanner(System.in);

System.out.print("Nhập số phần tử của mảng: ");
int n = sc.nextInt();

int[] mang = new int[n];
System.out.println("Nhập các phần tử của mảng:");
for (int i = 0; i < n; i++) {
System.out.print("Phần tử thứ " + (i + 1) + ": ");
mang[i] = sc.nextInt();
}

System.out.println("Các số chẵn trong mảng là:");
for (int i = 0; i < n; i++) {
if (mang[i] % 2 == 0) {
System.out.print(mang[i] + " ");
}
}
}
}



8.//nhập liên tục các số nguyên cho đên khi gặp số chia hết cho 4 thì cộng lại các số đã nhập
package inforstudent;
import java.util.Scanner;
public class bai8 {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int t=0;
int a;
do {
System.out.print("nhập a: ");
a=sc.nextInt();
t=t+a;
}
while(a % 4 != 0);
System.out.print("tổng: "+t);
}

}



9.//viết chương trình in ra các số nguyên tố từ m đến n(m<n)
package inforstudent;
import java.util.Scanner;
public class bai9 {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
//có số cụ thể
//int m=2,n=7;
//nt(m,n);
//hoặc nhập từ bàn phím
System.out.print("Nhập giá trị m: ");
int m = sc.nextInt();
System.out.print("Nhập giá trị n: ");
int n = sc.nextInt();
nt(m, n);
sc.close();
}
public static boolean kt(int n) {
if(n<2) {
return false;
}
for (int i=2;i<Math.sqrt(n);i++)
{
if (n % i==0) {
return false;
}
}
return true;
}
public static void nt(int m,int n) 
{
for (int j=m;j<n;j++)
if(kt(j)==true) 
{
System.out.print(j);
}
}
} 




10.//tính số mũ M mũ N
package inforstudent;
import java.util.Scanner;
public class tinhsomu {

public static void main(String[] args) { 
Scanner sc = new Scanner(System.in); 
System.out.print("Nhập M: ");
int m = sc.nextInt();
System.out.print("Nhập N: ");
int n = sc.nextInt();
long ketQua = tinhSoMu(m, n);
System.out.println(m + " mũ " + n + " = " + ketQua);
}

public static long tinhSoMu(int m, int n) {
long ketQua = 1;
for (int i = 1; i <= n; i++) {
ketQua *= m;
}
return ketQua;
}
}




11.//ct s=1+3+5+7...+n
package inforstudent;
import java.util.Scanner;
public class bai11 {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
System.out.print("Nhập giá trị n: ");
int n = sc.nextInt();
int t=0;
for(int i=1;i<=n;i++)
{
if (i % 2 !=0) {
t= t+1;
}
}
System.out.print(+t);
}

}



12.//ct s=1+2+3+...+n!
package inforstudent;
import java.util.Scanner;
public class bai12 {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
System.out.print("Nhập giá trị n: ");
int n = sc.nextInt();
int tgt=tonggt(n);
System.out.print("tổng gt số lẻ= "+tgt);
}
public static int sole(int N)
{
int gt=1;
for(int i=1;i<N;i++) {
gt*=i;
}
return gt;
}
public static int tonggt(int m) {
int s=0;
for (int i=1;i<=m;i+=2) {
s +=sole(i);
}
return s;
}
}
